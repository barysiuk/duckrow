# Test listing tracked folders

# No folders initially
exec duckrow folders
stdout 'No tracked folders'
! stderr .

# Add some folders
mkdir project-a
mkdir project-b
exec duckrow add project-a
exec duckrow add project-b

# List should show both
exec duckrow folders
stdout 'Tracked folders \(2\)'
stdout 'project-a'
stdout 'project-b'
